{
    "name": "symfony/framework-standard-edition",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": { "": "src/" },
	"classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ],
        "files": [
            "src/Naukri/JobPostingGatewayBundle/Resources/config/app/app_config_common.php",
            "src/Naukri/JobPostingGatewayBundle/Resources/config/app/app_config.php"
        ]
    },
    "require": {
 	"doctrine/doctrine-bundle": "~1.4",
	"doctrine/orm": "^2.4.8",
	"incenteev/composer-parameter-handler": "~2.0",
	"jms/di-extra-bundle": "^1.9",
	"jms/security-extra-bundle": "^1.6",
	"noiselabs/smarty-bundle": "2.0.x-dev",
	"php": ">=7.0.9",
	"sensio/distribution-bundle": "~4.0",
	"sensio/framework-extra-bundle": "^3.0.2",
	"sensio/generator-bundle": "~3.0",
	"symfony/assetic-bundle": "^2.8",
	"symfony/monolog-bundle": "^3.0.2",
	"symfony/swiftmailer-bundle": "~2.3,>=2.3.10",
	"symfony/symfony": "2.8.*",
	"twig/extensions": "1.*",
	"twig/twig": "^1.0||^2.0",
	"zendframework/zend-mail": "2.8.0",
	"zendframework/zend-validator": "2.10.1",
	"zendframework/zend-servicemanager": "3.3.0",
        "zendframework/zend-crypt":"3.2.1",
        "zendframework/zend-math":"3.0.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "~3.0",
        "symfony/phpunit-bridge": "~2.7"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.zendframework.com/"
        }
    ],
    "minimum-stability": "stable",
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    },
    "config": {
	"secure-http":false
    }
}

